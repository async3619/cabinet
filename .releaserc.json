{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": true
    }
  ],
  "ci": true,
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    [
      "semantic-release-pub",
      {
        "cli": "flutter",
        "publishPub": false
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "pubspec.yaml"
        ],
        "message": "chore(ðŸ“¦): ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ]
  ]
}